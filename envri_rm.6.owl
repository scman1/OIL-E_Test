  </owl:Class>
  <owl:Class rdf:about="#IV_Annotate">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#IV_Action"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >IV_Annotate</rdfs:label>
    <ENVRI:ENVRI_definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >add information to an object</ENVRI:ENVRI_definition>
  </owl:Class>
  <owl:Class rdf:ID="SV_DataDiscoveryAndAccess">
    <rdfs:subClassOf rdf:resource="#SV_CommunityBehavior"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/odp.6.owl#ev_isPerformedBy"/>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#SV_DataAccessSubsystem"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <ENVRI:ENVRI_definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Data Discovery and Access: A behaviour enabled by a Data Discovery and 
Access system that retrieves requested data from a data resource by 
using suitable search technology.</ENVRI:ENVRI_definition>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SV_DataDiscoveryAndAccess</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="CV_ProcessRequestServer">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#cv_fitsClient"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#CV_ProcessRequestClient"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#CV_ProcessRequest"/>
