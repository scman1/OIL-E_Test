    <rdfs:subClassOf rdf:resource="#CV_ServerInterface"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >CV_ExportMetadataServer</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#IV_MappedData">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#IV_PublishData"/>
