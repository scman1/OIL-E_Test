    <rdfs:subClassOf rdf:resource="#CV_ImportDataForCuration"/>
    <rdfs:subClassOf rdf:resource="#CV_ConsumerInterface"/>
  </owl:Class>
  <owl:Class rdf:about="#CV_VirtualLaboratory">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
