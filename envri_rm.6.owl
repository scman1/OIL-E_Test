  <owl:Class>
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#CV_ImportDataForCuration"/>
      <owl:Class rdf:about="#CV_Interface"/>
    </owl:unionOf>
  </owl:Class>
