    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#CV_CoordinateProcessServer"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#cv_providesServerInterface"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="CV_StageDataConsumer"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#cv_providesConsumerInterface"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#cv_providesServerInterface"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#CV_UpdateRecordsServer"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#CV_Object"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#cv_providesServerInterface"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#CV_RetrieveDataServer"/>
