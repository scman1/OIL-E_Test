    >CV_InterfaceType</rdfs:label>
    <rdfs:subClassOf rdf:resource="#CV_Interface"/>
    <ENVRI:ENVRI_definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Interfaces of different types such as client, server, consumer, producer interface.</ENVRI:ENVRI_definition>
  </owl:Class>
  <owl:Class rdf:about="#IV_AnnotatedData">
