    <rdfs:subClassOf rdf:resource="#CV_ServerInterface"/>
  </owl:Class>
  <owl:Class rdf:about="#SV_DataReplication">
    <rdfs:subClassOf rdf:resource="#SV_CommunityBehavior"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#SV_StorageAdministrator"/>
