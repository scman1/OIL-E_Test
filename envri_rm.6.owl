          <owl:ObjectProperty rdf:about="#iv_delegateTo"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#IV_DataQuery"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CV_ResolveIdentifierClient">
