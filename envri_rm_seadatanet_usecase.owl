    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#IV_DoDataMining -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#IV_DoDataMining">
