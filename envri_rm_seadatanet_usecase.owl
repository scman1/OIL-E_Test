        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;creates"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ProcessedData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;hasClassifier"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUsePhase"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Creating visual models which display data alpha-numerically, graphically, or geographically.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#IdentifiedData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#IdentifiedData">
