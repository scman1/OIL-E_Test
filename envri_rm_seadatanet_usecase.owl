    <!-- http://www.oil-e.net/ontology/rm-core.owl#Actor -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-core.owl#Actor">
        <rdfs:label>actor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#SV_Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#required_for"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#SV_Behaviour"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#hasPart"/>
                <owl:allValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#Actor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#memberOf"/>
                <owl:allValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#community"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An actor is a science object that takes an active role in the behaviours of a system. Generally, such actors represent people or computational agents.</dc:description>
    </owl:Class>
    


