    <!-- http://www.oil-e.net/ontology/rm-core.owl#CV_Thing -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-core.owl#CV_Thing">
        <rdfs:label>computational view thing</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
