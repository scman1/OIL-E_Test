                    <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&core;hasAttribute"/>
                        <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#IdentifiedState"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>Data which have been assigned a unique identifier.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ImportDataForCuration -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ImportDataForCuration">
