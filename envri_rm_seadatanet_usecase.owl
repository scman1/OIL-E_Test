<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY core "http://www.oil-e.net/ontology/rm-core.owl#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.oil-e.net/ontology/rm-archetypes.owl#"
     xml:base="http://www.oil-e.net/ontology/rm-archetypes.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:core="http://www.oil-e.net/ontology/rm-core.owl#">
    <owl:Ontology rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl">
        <rdfs:label xml:lang="en">ENVRI-RM ontology</rdfs:label>
        <dc:creator>Barbara Magagna</dc:creator>
        <rdfs:comment>Created within the ENVRI[+] project(s), EU FP7 283465 and H2020 654182.</rdfs:comment>
        <rdfs:comment>Ontology for describing the architecture and information flow of environmental science research infrastructures.</rdfs:comment>
        <dc:creator>Paul Martin</dc:creator>
        <owl:versionInfo>2.1 (January 2017)</owl:versionInfo>
        <dc:title>ENVRI-RM ontology</dc:title>
        <dc:description>An ontology for describing the architecture and information flow of environmental science research infrastructures, based on the ENVRI Reference Model (http://envri.eu/rm).</dc:description>
        <dc:language>en</dc:language>
        <dc:creator>Zhiming Zhao</dc:creator>
        <dc:subject xml:lang="en">environmental science research infrastructure</dc:subject>
        <owl:imports rdf:resource="http://www.oil-e.net/ontology/rm-core.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    


    <!-- http://www.oil-e.net/ontology/rm-core.owl#precondition -->

    <owl:AnnotationProperty rdf:about="&core;precondition"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#instantiates -->

    <owl:ObjectProperty rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#instantiates">
        <rdfs:label>instantiates</rdfs:label>
        <rdfs:domain rdf:resource="&core;CV_Object"/>
        <rdfs:range rdf:resource="&core;CV_Object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability -->

    <owl:ObjectProperty rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability">
        <rdfs:label>invokes capability</rdfs:label>
        <rdfs:range rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ComputationalCapability"/>
        <rdfs:domain rdf:resource="&core;CV_Object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability -->

    <owl:ObjectProperty rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability">
        <rdfs:label>provides capability</rdfs:label>
        <rdfs:range rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ComputationalCapability"/>
        <rdfs:domain rdf:resource="&core;CV_Object"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AAAIService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AAAIService">
        <rdfs:label>AAAI service</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AuthoriseActionCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>AAAI services handle authorisation requests and authentication of users before they can proceed with any privileged activities.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AcquireIdentifier -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AcquireIdentifier">
        <rdfs:label>acquire identifier [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_SimpleOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AcquireIdentifierCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AcquireIdentifierCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">is used to request a new persistent identifier to be associated with the data being transferred.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AcquisitionService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AcquisitionService">
        <rdfs:label>acquisition service</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ConfigureControllerCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#UpdateRegistryCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PrepareDataTransferCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Acquisition services are responsible for ensuring that any data is delivered into the infrastructure in accordance with current policies.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotateData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotateData">
        <rdfs:label>annotate data [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_SimpleOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotateDataCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotateDataCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">is used to request annotation of data held within the data curation subsystem of a research infrastructure.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotatedData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotatedData">
        <rdfs:label>annotated data</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&core;hasAttribute"/>
                        <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotatedState"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>Data that are connected to concepts, describing their meaning.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotatedMetadata -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotatedMetadata">
        <rdfs:label>annotated metadata</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&core;hasAttribute"/>
                        <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotatedState"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>Metadata that are connected to concepts, describing their meaning.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotationService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotationService">
        <rdfs:label>annotation service</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AnnotateDataCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#UpdateRecordCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#UpdateCatalogueCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Annotation services are concerned with the updating of records (such as data-sets) and catalogues in response to user annotation requests.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#AuthoriseAction -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#AuthoriseAction">
        <rdfs:label>authorise action [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_SimpleOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AuthoriseActionCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AuthoriseActionCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">Is used to retrieve authorisation for any restricted interactions with the data acquisition subsystem.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#BackendObject -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#BackendObject">
        <rdfs:label>backend object</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_PersistentObject"/>
        <dc:description>Backend objects are computational objects which encompass the RI&apos;s systems and resources provided for acquiring, preserving, publishing, and processing research data and derived data products.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#BackupData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#BackupData">
        <rdfs:label>backup data</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
        <dc:description>A copy of (persistent) data so it may be used to restore the original after a data loss event.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#BrokerObject -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#BrokerObject">
        <rdfs:label>broker object</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_PersistentObject"/>
        <dc:description>Broker objects act as intermediaries for access to data held within the data store and facilitate performing semantic interpretation and routing of queries.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#BrokeredDataExport -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#BrokeredDataExport">
        <rdfs:label>brokered data export [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_ComplexOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataExporter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Exporting data out of a research infrastructure entails retrieving data from the data curation subsystem and delivering it to an external resource. This process must be brokered by the data use and data publishing subsystems.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#BrokeredDataImport -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#BrokeredDataImport">
        <rdfs:label>brokered data import [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_ComplexOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataImporter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Importing data from sources other than the acquisition network requires that the import be brokered by the publishing subsystem before data can be delivered into the data curation subsystem.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#CalibrateInstrument -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#CalibrateInstrument">
        <rdfs:label>calibrate instrument [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_SimpleOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#CalibrateInstrumentCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#CalibrateInstrumentCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">is used to calibrate the reading of data by instruments based (in principle) on scientific analysis of data output. This interface can also be used to monitor activity on a given instrument.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#CapacityManager -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#CapacityManager">
        <rdfs:label>capacity manager</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An active role, which is a person who manages and ensures that the IT capacity meets current and future business requirements in a cost-effective manner.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#CatalogueService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#CatalogueService">
        <rdfs:label>catalogue service</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#QueryDataCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#QueryResourceCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#UpdateCatalogueCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ExportMetadataCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Catalogue services are concerned with the cataloguing of metadata and other characteristic data associated with datasets stored within the infrastructure.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#CatalogueSystem -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#CatalogueSystem">
        <rdfs:label>catalogue system</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, a catalogue system is a special type of storage system designed to support building logical structures for classifying data and metadata.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Citation -->

