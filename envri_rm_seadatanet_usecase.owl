                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ImportDataForCurationCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ImportDataForCurationCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">receives data packaged for curation within the associated data store.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#InstrumentController -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#InstrumentController">
        <rdfs:label>instrument controller</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ComponentObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#CalibrateInstrumentCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RetrieveDataCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ConfigureControllerCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DeliverRawDataCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Instrument controllers encapsulate the accessible functionalities of instruments and other raw data sources out in the field.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Investor -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Investor">
        <rdfs:label>investor</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#User"/>
        <dc:description>An active role, which is a person who makes use of the data and application service for predicting markets so as to make business decisions on producing related commercial products.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Journalist -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Journalist">
        <rdfs:label>journalist</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#User"/>
        <dc:description>An active role, which is a person or organisation interested in understanding the knowledge delivered by an environmental science research infrastructure, or discovering and exploring the knowledge base enabled by the research infrastructure.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#LifecyclePhase -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#LifecyclePhase">
        <rdfs:label>lifecycle phase</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;RM_Classifier"/>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#LocalConcept -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#LocalConcept">
        <rdfs:label>local concept</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Concept"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#LocalConceptualModel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <core:precondition>Concepts for all needed information about the measurement or observation (design, location selected, selection methods, devices used, methods applied, additional information needed, code lists used for description, etc.) are created.</core:precondition>
        <core:precondition>Data are annotated with local concepts.</core:precondition>
        <dc:description>A concept within a local conceptual model.</dc:description>
        <core:precondition>Local concepts are mapped to global concepts.</core:precondition>
        <core:precondition>Concepts are linked to the data = data are semantically annotated.</core:precondition>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#LocalConceptualModel -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#LocalConceptualModel">
        <rdfs:label>local conceptual model</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ConceptualModel"/>
        <core:precondition>A local conceptual model describing the meaning of the data is established.</core:precondition>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#MappedData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#MappedData">
        <rdfs:label>mapped data</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&core;hasAttribute"/>
                        <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#MappedState"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>Data that are mapped to a certain conceptual model.</dc:description>
        <core:precondition>Data are annotated with local concepts.</core:precondition>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#MappingRule -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#MappingRule">
        <rdfs:label>mapping rule</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata"/>
        <dc:description>Configuration directives used for model-to-model transformation.</dc:description>
        <core:precondition>Mapping rules between global concepts and local concepts are created.</core:precondition>
        <core:precondition>Local concepts are mapped to global concepts.</core:precondition>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#MeasurementModelDesigner -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#MeasurementModelDesigner">
        <rdfs:label>measurement model designer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;memberOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionCommunity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An active role, which is a person who designs the measurements and monitoring models based on the requirements of environmental scientists.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#MeasurementResult -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#MeasurementResult">
        <rdfs:label>measurement result</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
        <dc:description>Quantitative, qualitative, or cataloguing determinations of magnitude, dimension, and uncertainty to the outputs of observation instruments, sensors, sensor networks, human observers and observer networks.</dc:description>
        <core:precondition>The measurement description including description of used device is recorded as long as information on that is available.</core:precondition>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Measurer -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Measurer">
        <rdfs:label>measurer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;memberOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionCommunity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An active role, which is a person who determines the ratio of a physical quantity (such as a length, time, temperature etc.), to a unit of measurement (such as the meter, second or degree Celsius).</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata">
        <rdfs:label>metadata</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;hasAttribute"/>
                <owl:allValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataState"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataCatalogue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Data about data, in scientific applications is used to describe, explain, locate, or make it easier to retrieve, use, or manage a data resource.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataCatalogue -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataCatalogue">
        <rdfs:label>metadata catalogue</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData"/>
        <dc:description>A collection of metadata, usually established to make the metadata available to a community. A metadata catalogue can be exposed through an access service.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataHarvester -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataHarvester">
        <rdfs:label>metadata harvester</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;memberOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingCommunity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, which is a system or service collecting metadata which supports the construction/selection of a global conceptual model and the production of mapping rules</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataState -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#MetadataState">
        <rdfs:label>metadata state</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;IV_State"/>
        <dc:description>Metadata state is a refinement of data state applicable to data used as metadata in the system.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ModellingAndSimulationService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ModellingAndSimulationService">
        <rdfs:label>modelling and simulation service</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Service"/>
        <dc:description>A service providing functionality for modelling and simulation.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Observer -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Observer">
        <rdfs:label>observer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;memberOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionCommunity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An active role, which is a person who receives knowledge of the outside world through his senses, or records data using scientific instruments.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#PIDGenerator -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#PIDGenerator">
        <rdfs:label>pid service</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, a public system or service which can generate and assign persistent global unique identifiers (PIDs) to sets of digital objects. The PID Service also maintains a public registry of PIDs for digital objects.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#PIDManager -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#PIDManager">
        <rdfs:label>PID manager</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;memberOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationCommunity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, a system or service that assigns persistent global unique identifiers to data and metadata products. The Manager invokes a external entity, the PID Service, to obtain the PIDs. The manager maintains a local catalogue of PIDs that are being used to reference data and metadata. If the data or metadata in the RI change location or are removed, the PID manager updates this information locally and informs the PID Service.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#PIDService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#PIDService">
        <rdfs:label>PID service</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#AcquireIdentifierCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ResolveIdentifierCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>PID services provide globally-readable persistent identifiers (PIDs) to infrastructure entities, mainly datasets, that may be cited by the community.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#PersistentData">
