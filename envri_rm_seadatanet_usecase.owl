    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-core.owl#CV_ComplexOperation">
        <rdfs:label>complex operation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_Operation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#providedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_BindingObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_SimpleOperation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#providedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_BindingObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A complex operation represents a remote functionality invoked by one computation object onto a binding object created to broker interaction between more than two computational objects.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-core.owl#CV_DataStream -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-core.owl#CV_DataStream">
        <rdfs:label>data stream</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#RM_Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#consumedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_Object"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#consumedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_Object"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#producedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_Object"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#hasClassifier"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#ComputationalViewpoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#producedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-core.owl#CV_Object"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A data stream is a computational entity representing the transferral of data from one location to another, brokered by computational objects installed at each location.</rdfs:comment>
    </owl:Class>
    


