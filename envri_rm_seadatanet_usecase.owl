        <dc:description>An active role, which is a person who develops and maintains the research infrastructure.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#EnvironmentalScientist -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#EnvironmentalScientist">
