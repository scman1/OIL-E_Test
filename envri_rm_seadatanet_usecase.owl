                    <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&core;hasAttribute"/>
                        <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RawState"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>Established metadata, which are not yet registered. In general, they are not shareable in this state.</dc:description>
        <core:precondition>Raw data is persisted in a way that further information can be added: it must have a minimum set of identifiers and it must allow links.</core:precondition>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#RegisteredBackup -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#RegisteredBackup">
        <rdfs:label>registered backup</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#BackupData"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&core;hasAttribute"/>
                        <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RegisteredState"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>Backup data that has been registered with some registry service or catalogue.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#RegisteredMetadata -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#RegisteredMetadata">
        <rdfs:label>registered metadata</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&core;hasAttribute"/>
                        <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RegisteredState"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description>Metadata which are inserted into a metadata catalogue.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#RequestData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#RequestData">
        <rdfs:label>request data [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_SimpleOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RequestDataCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RequestDataCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">is used to make requests of the research infrastructure pertaining to curated datasets.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ResearchInfrastructure -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ResearchInfrastructure">
        <rdfs:label>research infrastructure</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <dc:description>A conglomeration of research resources providing some subset of data acquisition, data curation, data publishing, data processing and data use functionality to a research community.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Researcher -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Researcher">
        <rdfs:label>researcher</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#User"/>
        <dc:description>An active role, which is a person who makes use of the data and application services to conduct scientific research.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ResolveIdentifier -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ResolveIdentifier">
        <rdfs:label>resolve identifier [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_SimpleOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ResolveIdentifierCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ResolveIdentifierCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">Is used to resolve identifiers.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#RetrieveData -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#RetrieveData">
        <rdfs:label>retrieve data [operation]</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_SimpleOperation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;invokedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#invokesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RetrieveDataCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;providedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&core;CV_Object"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#RetrieveDataCapability"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="&xsd;string">is used to request a new persistent identifier to be associated with the data being transferred.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_CoCreate -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_CoCreate">
        <rdfs:label>co-create [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="&core;Actor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by active roles which entails the design and planning of activities for the collection, preservation, analysis or publishing of research data in partnership with different communities.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_Collaborate -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_Collaborate">
        <rdfs:label>collaborate [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="&core;Actor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by active roles which entails assisting/participating in some of the phases of the collection, preservation, analysis or publishing of research data.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_ComposeService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_ComposeService">
        <rdfs:label>compose service [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceProvider"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceConsumer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Service Provider to combine multiple services which can be achieved by either Choreography or Orchestration. Service Choreography is a collaboration between Service Providers and Service Consumers. Service Orchestration is the behaviour that a Service Provider performs internally to realise a service that it provides.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_Contribute -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_Contribute">
        <rdfs:label>contribute [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="&core;Actor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by active roles which entails directly collecting, preserving, analysing, or publishing research data held by the RI, according to a predefined protocol.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_CoordinateService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_CoordinateService">
        <rdfs:label>coordinate service [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceProvider"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Service Provider to coordinate the actions of distributed applications in order to reach consistent agreement on the outcome of distributed transactions.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_CustomiseProcessingEnvironment -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_CustomiseProcessingEnvironment">
        <rdfs:label>customise processing environment [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ProcessingEnvironmentPlanner"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a processing environment planner to enable a Data Processing Subsystem to prepare customised infrastructure and service platforms for managing specific data processing applications optimally, including the planning, provisioning and deployment sub-activities.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataAnnotation -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataAnnotation">
        <rdfs:label>data annotation [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticCurator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Semantic Curator which supports the linking of data and metadata with a local conceptual model.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataCitation -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataCitation">
        <rdfs:label>data citation [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PIDManager"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a PID Manager that assigns an accurate, consistent and standardised reference to a data object, in the same way as researchers routinely provide a bibliographic reference to printed resources. The RI publishing the data can define the citation contents such as authors, and dates for different citation styles.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataCollection -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataCollection">
        <rdfs:label>data collection [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Measurer"/>
                            <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Observer"/>
                            <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Sensor"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCollector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Data Collector that control and monitor the collection of the digital values from a sensor instrument (or a human sensor such as a Measurer or a Observer), associating consistent timestamps and necessary metadata.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataDiscoveryAndAccess -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataDiscoveryAndAccess">
        <rdfs:label>data discovery and access [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour enabled by a Data Discovery and Access System that retrieves requested data from a data resource by using suitable search technology.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataIdentification -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataIdentification">
        <rdfs:label>data identification [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PIDManager"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a PID manager which provides a unique PID for data and metadata being curated.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataPreservation -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataPreservation">
        <rdfs:label>data preservation [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Data Curator that deposits (over long-term) the data and metadata or other supplementary data and methods according to specified policies, and makes them accessible on request.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataProductGeneration -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataProductGeneration">
        <rdfs:label>data product generation [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Data Curator that processes data against requirement specifications and standardised formats and descriptions.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataPublication -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataPublication">
        <rdfs:label>data publication [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingBehaviour"/>
        <dc:description>A behaviour that provides clean, well-annotated, anonymity-preserving datasets in a suitable format, and by following specified data-publication and sharing policies, to make the datasets accessible publicly or to those who agree to certain conditions of use, and to individuals who meet certain professional criteria.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataQualityChecking -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataQualityChecking">
        <rdfs:label>data quality checking [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Data Curator that detects and corrects (or removes) corrupt, inconsistent or inaccurate records from data sets.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataReplication -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DataReplication">
        <rdfs:label>data replication [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#StorageAdministrator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Storage Administrator that creates, deletes and maintains the consistency of copies of a data set on multiple storage devices.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DescribeService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DescribeService">
        <rdfs:label>describe service [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceProvider"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Service Provider to provide the information needed in order to use a service.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DesignExperiment -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DesignExperiment">
        <rdfs:label>design experiment [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#EnvironmentalScientist"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Environmental Scientist that designs the scientific experiment which motivates the data acquisition activities.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DesignMeasurementModel -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_DesignMeasurementModel">
        <rdfs:label>design measurement model [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#MeasurementModelDesigner"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Measurement Model Designer that designs the measurement or monitoring model based on scientific requirements.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_InstrumentCalibration -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_InstrumentCalibration">
        <rdfs:label>instrument calibration [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Technician"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Sensor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Technician that controls and records the process of aligning or testing a sensor against dependable standards or specified verification processes.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_InstrumentConfiguration -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_InstrumentConfiguration">
        <rdfs:label>instrument configuration [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Technician"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;creates"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Sensor"/>
                            <rdf:Description rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SensorNetwork"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Technician that sets up a sensor or a sensor network.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_MetadataHarvesting -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_MetadataHarvesting">
        <rdfs:label>metadata harvesting [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a metadata harvester to gather metadata from data objects in order to construct catalogues of the available information.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_RegisterService -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_RegisterService">
        <rdfs:label>register service [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceProvider"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Service Provider to make the service visible to Service Consumers by registering it in a service registry.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_SelectOrBuildConceptualModel -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_SelectOrBuildConceptualModel">
        <rdfs:label>select or build conceptual model [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticCurator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour performed by a Semantic Curator which supports the annotation of data and metadata.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_SemanticHarmonisation -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_SemanticHarmonisation">
        <rdfs:label>semantic harmonisation [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticMediator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour enabled by a Semantic Mediator that unifies similar data (knowledge) models based on the consensus of collaborative domain experts to achieve better data (knowledge) reuse and semantic interoperability.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserBehaviourTracking -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserBehaviourTracking">
        <rdfs:label>user behaviour tracking [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#User"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour enabled by a Community Support System to track the Users. If the research infrastructure has identity management, authorisation mechanisms, accounting mechanisms, for example, a Data Access (Sub)system is provided, then the Community Support System either include these or work well with them.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserGroupWorkSupport -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserGroupWorkSupport">
        <rdfs:label>user group work support [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour enabled by a Community Support System to support controlled sharing, collaborative work and publication of results, with persistent and externally citable PIDs.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserProfileManagement -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserProfileManagement">
        <rdfs:label>user profile management [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour enabled by a Community Support System to support persistent and mobile profiles, where profiles will include preferred interaction settings, preferred computational resource settings, and so on.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserWorkingRelationshipsManagement -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserWorkingRelationshipsManagement">
        <rdfs:label>user working relationships management [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour enabled by a Community Support System to support a record of working relationships, (virtual) group memberships and friends.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserWorkingSpaceManagement -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SV_UserWorkingSpaceManagement">
        <rdfs:label>user working space management [behaviour]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseBehaviour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;requires"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataUseSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A behaviour enabled by a Community Support System to support work spaces that allow data, document and code continuity between connection sessions and accessible from multiple sites or mobile smart devices.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ScienceGateway -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ScienceGateway">
        <rdfs:label>science gateway</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#PresentationObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#instantiates"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#VirtualLaboratory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A science gateway object encapsulates the functions required to interact with a research infrastructure from outside with the objects provided for data acquisition, data curation, data brokering and data processing. A science gateway should be able to provide virtual &apos;laboratories&apos; for authorised agents to interact with and possibly configure many of the science functions of a research infrastructure. A science gateway is also known as a Virtual Research Environment.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Scientist -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Scientist">
        <rdfs:label>scientist</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#User"/>
        <dc:description>An active role, which is a person who makes use of the data and application services to conduct scientific research.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticBroker -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticBroker">
        <rdfs:label>semantic broker</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#BrokerObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#TranslateRequestCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#UpdateModelCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Semantic broker objects perform semantic interpretation.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticCurator -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticCurator">
        <rdfs:label>semantic curator</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;memberOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataCurationCommunity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An active role, which is a person who designs and maintains local and global conceptual models and uses those models to annotate the data and metadata.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticLaboratory -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticLaboratory">
        <rdfs:label>semantic laboratory</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#VirtualLaboratory"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#providesCapability"/>
                <owl:hasValue rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#UpdateModelCapability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Semantic laboratories are used to interact with the semantic models used by a research infrastructure to interpret datasets and characteristic (meta)data.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticMediator -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SemanticMediator">
        <rdfs:label>semantic mediator</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;Actor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;memberOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataPublishingCommunity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, which is a system or middleware facilitating semantic mapping (i.e., executing mapping and translation rules), discovery and integration of heterogeneous data.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Sensor -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Sensor">
        <rdfs:label>sensor</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#SensorNetwork"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, which is a converter that measures a physical quantity and converts it into a signal which can be read by an observer or by an (electronic) instrument.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SensorBackend -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SensorBackend">
        <rdfs:label>sensor [backend]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#BackendObject"/>
        <dc:description>A converter that measures a physical quantity and converts it into a signal which can be read by an observer or by an (electronic) instrument.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SensorNetwork -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SensorNetwork">
        <rdfs:label>sensor network</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataAcquisitionSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, which is a network consisting of distributed autonomous sensors to monitor physical or environmental conditions.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SensorNetworkBackend -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SensorNetworkBackend">
        <rdfs:label>sensor network [backend]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#BackendObject"/>
        <dc:description>A network consisting of distributed sensors which monitor physical or environmental conditions.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#Service -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#Service">
        <rdfs:label>service</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, in which a  functionality for processing data is made available for general use.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceConsumer -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceConsumer">
        <rdfs:label>service consumer</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Either an active or a passive role, which is an entity using the services provided.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceDescription -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceDescription">
        <rdfs:label>service description</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata"/>
        <dc:description>Description of services and processes available for reuse. The description is needed to facilitate usage. The service description usually includes a reference to a service or process making it available for reuse within a research infrastructure or within an open network like the Internet. Usually such descriptions include the accessibility of the service, the description of the interfaces, the description of behavior and/or implemented algorithms. Such descriptions are usually done along service description standards (e.g. WSDL, web service description language). Within some service description languages, semantic descriptions of the services and/or interfaces are possible (e.g. SAWSDL, Semantic Annotations for WSDL).</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceObject -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceObject">
        <rdfs:label>service object</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;CV_PersistentObject"/>
        <dc:description>Service objects offer programmatic access to distributed systems and resources (internal and external).</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceProvider -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceProvider">
        <rdfs:label>service provider</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Either an active or a passive role, which is an entity providing the services to be used.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceRegistry -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceRegistry">
        <rdfs:label>service registry</rdfs:label>
        <rdfs:subClassOf rdf:resource="&core;resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&core;partOf"/>
                <owl:someValuesFrom rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#DataProcessingSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A passive role, which is an information system for registering services.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceRegistryBackend -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#ServiceRegistryBackend">
        <rdfs:label>service registry [backend]</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#BackendObject"/>
        <dc:description>An information system for registering services.</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SpecificationOfInvestigationDesign -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SpecificationOfInvestigationDesign">
        <rdfs:label>specification of investigation design</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.oil-e.net/ontology/rm-archetypes.owl#Metadata"/>
        <dc:description>This is the background data needed to understand the overall goal of the measurement or observation. It could be the sampling design of observation stations, the network design, the description of the setup parameters (interval of measurements) and so on. It usually contains important data for the allowed evaluations of research results (e.g. the question of whether a sampling design was done randomly or by stratification determines which statistical methods can be applied).</dc:description>
    </owl:Class>
    


    <!-- http://www.oil-e.net/ontology/rm-archetypes.owl#SpecificationOfMeasurementsOrObservations -->

    <owl:Class rdf:about="http://www.oil-e.net/ontology/rm-archetypes.owl#SpecificationOfMeasurementsOrObservations">
